}

type GetAllResource {
    ClusterName: String
    Namespace: String
    Kind: String
    Resource: String
    Age: String
    EventTime: String
    ExpiryDate: String
}

type DeletedAPI {
    ClusterName: String
    ObjectName: String
    Group: String
    Kind: String
    Version: String
    Name: String
    Deleted: Boolean
    Scope: String
    EventTime: String
    ExpiryDate: String
}

type DeprecatedAPI {
    ClusterName: String
    ObjectName: String
