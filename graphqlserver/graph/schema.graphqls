    name: String
    vulId: String
    vulVendorIds: String
    vulPkgId: String
    vulPkgName: String
    vulPkgPath: String
    vulInstalledVersion: String
    vulFixedVersion: String
    vulTitle: String
    vulSeverity: String
    vulPublishedDate: String
    vulLastModifiedDate: String
    expiryDate: String
}

type Kubescore {
    id: ID!
